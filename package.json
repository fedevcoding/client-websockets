{
  "name": "client-websockets",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.0.2",
  "description": "A multi-types websockets library.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "keywords": [
    "websockets",
    "typescript",
    "ws",
    "polling",
    "polling-websockets",
    "reconnecting-websockets"
  ],
  "repository": {
    "url": "https://github.com/fedevcoding/client-websockets",
    "type": "git"
  },
  "scripts": {
